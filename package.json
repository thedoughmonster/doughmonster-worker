{
  "name": "doughmonster-worker",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "dev:ui": "vite dev",
    "check": "tsc --noEmit",
    "build": "npm run build:worker && npm run build:ui",
    "build:worker": "tsc -p tsconfig.build.json",
    "build:ui": "vite build",
    "preview:ui": "vite preview",
    "test": "rm -rf dist && npm run build --silent && node --test tests/*.mjs"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20251008.0",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.2",
    "typescript": "5.6.3",
    "vite": "5.4.7",
    "wrangler": "4.42.2"
  }
}
